#include<bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--) {
        string s;
        cin >> s;
        int hash[27] = {0};
        for(int i=0;i<s.length();i++) {
            hash[s[i]-'a']++;
            if(hash[s[i]-'a']==1) cout << s[i];
        }
        cout << endl;
    }
}
